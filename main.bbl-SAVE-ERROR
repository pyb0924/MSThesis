      \strng{namehash}{5c18c2ae9611439129bc457548d141d7}
      \strng{fullhash}{2d243087132368224cdd1598b72f5b85}
      \strng{bibnamehash}{991b5a7c94f132de6b4ac180320a7a8e}
      \strng{authorbibnamehash}{991b5a7c94f132de6b4ac180320a7a8e}
      \strng{authornamehash}{5c18c2ae9611439129bc457548d141d7}
      \strng{authorfullhash}{2d243087132368224cdd1598b72f5b85}
      \strng{nameabibnamehash}{991b5a7c94f132de6b4ac180320a7a8e}
