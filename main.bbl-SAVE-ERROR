      \strng{namehash}{37a8ace05e695ce05255b09dfc98563f}
      \strng{fullhash}{3dc67b3d80056761a22e52513704b676}
      \strng{bibnamehash}{62dc046fddece6aa2a43e9495acdf6f6}
      \strng{authorbibnamehash}{62dc046fddece6aa2a43e9495acdf6f6}
      \strng{authornamehash}{37a8ace05e695ce05255b09dfc98563f}
      \strng{authorfullhash}{3dc67b3d80056761a22e52513704b676}
      \strng{nameabibnamehash}{62dc046fddece6aa2a43e9495acdf6f6}
