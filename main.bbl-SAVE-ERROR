      \strng{namehash}{3ee88ef08d510834eae24497815ada84}
      \strng{fullhash}{eafe1c69436e23f2ebba28ef6f10a66d}
      \strng{bibnamehash}{ae8732154e9744109726eeffbaa0edde}
      \strng{authorbibnamehash}{ae8732154e9744109726eeffbaa0edde}
      \strng{authornamehash}{3ee88ef08d510834eae24497815ada84}
