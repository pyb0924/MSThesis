      \strng{namehash}{8fd7339885b9605f5442e54daa89b3d4}
      \strng{fullhash}{8ad3fd850e17abc61ecc0e8aeac76a51}
      \strng{bibnamehash}{d7d316efedc12d484e4d2a5e0191f125}
