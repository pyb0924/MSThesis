      \strng{namehash}{48024679b20a8312513a1a26190b918d}
      \strng{fullhash}{566a07b9d6de9b8c6dd423b009321739}
      \strng{bibnamehash}{fe2e9fd66f407461e956019bfdbd6b9b}
      \strng{authorbibnamehash}{fe2e9fd66f407461e956019bfdbd6b9b}
      \strng{authornamehash}{48024679b20a8312513a1a26190b918d}
